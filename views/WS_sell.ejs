    <div>
      <!-- Links start -->
      <div>
        <ul class="nav nav-tabs">
          <li class="nav-item">
            <a class="nav-link" aria-current="page" href="workspace_page"
              >Buy Property</a
            >
          </li>
          <li class="nav-item">
            <a class="nav-link active" href="#">Sell Property</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="WS_rent">Rent Management</a>
          </li>
        </ul>
      </div>
      <!-- Link ends -->
      <!-- crads start -->
      <div class="row" style="padding: 5rem;padding-top: 1rem;">
        <div class="col-md-6" >
          <span class=""
            >
                <h5>Add a house for selling</h5>
                <form action="/WS_sell" method="post">
                    <div class="mb-3">
                        <label for="Name_person" class="form-label">Contact Person full name</label>
                        <input type="text" name="Name_person" id="Name_person" class="form-control">
                    </div>
                    <div class="mb-3">
                        <label for="State" class="form-label">State</label>
                        <input type="text" name="State" id="State" class="form-control">
                    </div>
                    <div class="mb-3">
                        <label for="city_name" class="form-label">City Name</label>
                        <input type="text" name="city_name" id="city_name" class="form-control">
                    </div>
                    <div class="mb-3">
                        <label for="Unique_id" class="form-label">House Number and Residency name</label>
                        <input type="text" name="Unique_id" id="Unique_id" class="form-control">
                    </div>
                    <div class="mb-3">
                        <label for="Address" class="form-label">Full Address</label>
                        <input type="text" name="Address1" id="Address1" class="form-control">
                        <input type="text" name="Address2" id="Address2" class="form-control">
                        <input type="text" name="Address3" id="Address3" class="form-control">
                    </div>
                    <div class="mb-3">
                        <label for="size" class="form-label">Total size</label>
                        <input type="number" name="size" id="size" class="form-control" placeholder="Square feet">
                    </div>
                    <div class="mb-3">
                        <label for="additional_info" class="form-label">Additional information</label>
                        <input type="text" name="additional_info" id="additional_info" class="form-control"   
                        maxlength="1000" style="height: 50rem;"
                        placeholder="Additional info">
                    </div>
                    <div class="mb-3">
                      <label for="Img_url" class="form-label">Image URL:</label>
                      <input type="text" name="Img_url" class="form-control">
                    </div>
                    <div class="mb-3">
                        <label for="Price" class="form-label">Price</label>
                        <input type="number" name="Price" id="Price" class="form-control">
                        <div id="Price_in_rupees" class="form-text">Please enter your Price in Rupees</div>
                    </div>
                    <div class="mb-3">
                        <label for="zip_code" class="form-label">Zip code</label>
                        <input type="text" pattern="[0-9]{5}" name="zip_code" id="zip_code" class="form-control">
                    </div>
                    <div class="mb-3">
                        <label for="BHK" class="form-label">BHK</label>
                        <input type="number" name="BHK" id="BHK" class="form-control">
                    </div>
                    <div class="mb-3">
                        <label for="phoneNumber" class="form-label">Phone Number for contact</label>
                        <input type="tel" name="phoneNumber" id="phoneNumber" class="form-control">
                        <div id="phoneNumber" class="form-text">Your Phone number will be displayed to everyone.    </div>
                    </div>
                    <button type="submit" class="btn btn-success">Submit</button>
                  </form>
            </span>
        </div>
        <div class="col-md-6" style="padding: 5rem;padding-top: 1rem;">
            <h5>Remove a house form selling</h5>
            <form action="/delete_property" method="post">
              <div class="mb-3">
                  <label for="removeState" class="form-label">State</label>
                  <input type="text" name="removeState" id="removeState" class="form-control" required>
              </div>
              <div class="mb-3">
                  <label for="removeCity" class="form-label">City Name</label>
                  <input type="text" name="removeCity" id="removeCity" class="form-control" required>
              </div>
              <div class="mb-3">
                  <label for="removeUniqueID" class="form-label">House Number and Residency name</label>
                  <input type="text" name="removeUniqueID" id="removeUniqueID" class="form-control" required>
              </div>
              <button type="submit" class="btn btn-danger">Remove Property</button>
            </form>
        </div>
      </div>
      <!-- card end -->
      <!-- Footer starts -->
      <div class="container">
        <footer
          class="d-flex flex-wrap justify-content-between align-items-center py-3 my-4 border-top"
        >
